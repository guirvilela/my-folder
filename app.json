{
  "expo": {
    "name": "myfolder",
    "slug": "myfolder",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#257F49"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir ${PRODUCT_NAME} acessar suas fotos",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "android": {
      "package": "com.guirvilela.myfolder"
    }
    // "ios": {
    //   "supportsTablet": true,
    //    "splash": {
    //   "image": "./assets/images/splash-icon.png",
    //   "resizeMode": "contain",
    //   "backgroundColor": "#ffffff"
    // }
  }
}